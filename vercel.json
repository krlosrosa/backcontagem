{
    "version": 2,
    "builds": [
        {
          "src": "package.json",
          "use": "@vercel/node"
        }
      ],
    "outputDirectory": "dist",
    "routes": [
        {
          "src": "/(.*)",
          "dest": "dist/main/index.js"
        }
      ],
    "env": {
      "NODE_ENV": "production"
    }
  }
  