{
    "version": 2,
    "buildCommand": "npm run build",
    "builds": [
        { "src": "dist/main/index.js", "use": "@vercel/node" }
    ],
    "outputDirectory": "dist",
    "routes": [
        {
          "src": "/(.*)",
          "dest": "dist/main/index.js"
        }
      ],
    "env": {
      "NODE_ENV": "production"
    }
  }
  