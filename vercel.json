{
    "version": 2,
    "builds": [
        { "src": "*.js", "use": "@vercel/node" }
    ],
    "functions": {
        "api/test.js": {
          "memory": 3009,
          "maxDuration": 30
        },
    "outputDirectory": "dist",
    "routes": [
        {
          "src": "/(.*)",
          "dest": "dist/main/index.js"
        }
      ],
    "env": {
      "NODE_ENV": "production"
    }
  }
  