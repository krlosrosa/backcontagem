{
    "version": 2,
    "buildCommand": "npm run build",
    "functions": {
        "dist/main/index.js": {
          "memory": 3009,
          "maxDuration": 30
        }
    },
    "outputDirectory": "dist",
    "routes": [
        {
          "src": "/(.*)",
          "dest": "dist/main/index.js"
        }
      ],
    "env": {
      "NODE_ENV": "production"
    }
  }
  